                                                                            <div class="input-group-btn-vertical">
                                                                                <button class="btn btn-default"><i class="fas fa-angle-up"></i></button>
                                                                                <button class="btn btn-default"><i class="fas fa-angle-down"></i></button>
                                                                            </div>
                                                                        </div>
                                                                        <span style="margin-left: 4px;">
                                                                            <strong>&times;</strong>
                                                                            <span class="cart-price"><%=this.CurrencySymbol %><%# String.Format(CultureInfo.InvariantCulture,"{0:0,0.00}", Eval("Price"))%></span>
                                                                            <asp:LinkButton ID="btnUpdate" runat="server" Style="float: right; margin-top: 5px;" CommandName="qtyUpdate"><i class="fa fa-sync"></i></asp:LinkButton></span>
                                                                    </td>
                                                                    <td>
                                                                        <asp:LinkButton ID="lnkDelete" runat="server" CommandName="qtyDelete" CssClass="edit"><i class="far fa-trash-alt"></i></asp:LinkButton>
                                                                    </td>
                                                                </tr>
                                                            </ItemTemplate>
                                                            <FooterTemplate>
                                                                </tbody>
                                                        </table>
                                                                <div class="row cart-lst-btn fright" style="margin-right: -10px; margin-top: 5px;">
                                                                    <a href="<%= baseUrl %>cart.aspx" class="btn btn-primary btn-md filled">View Cart</a>
                                                                    <a href="<%= baseUrl %>checkout.aspx" class="btn btn-primary btn-md filled">Checkout</a>
                                                                </div>
                                                            </FooterTemplate>
                                                        </asp:Repeater>
                                                    </div>
                                                </div>
                                            </li>

