            <script type="text/javascript">

                //const currentlocation = location.href;
                //const menuitem = document.querySelectorAll('a');
                //const menulenth = menuitem.length;
                //for (let i = 0; i < menulenth; i++) {
                //    if (menuitem[i].href === currentlocation) {
                //        menuitem[i].className = "active"
                //    }
                //}

                //$(window).scroll(function () {
                //    sessionStorage.scrollTop = $(this).scrollTop();
                //});

                //$(document).ready(function () {
                //if (sessionStorage.scrollTop != "undefined") {
                //    $(window).scrollTop(sessionStorage.scrollTop);
                //}
                //});
                $(document).ready(function () {

                    $(".offers_navbtn.dropdown .btn-account").click(function () {
                        $(this).parent('li').children('ul.dropdown-menu').toggle();
                    });

                    $(".country").parent('a').click(function () {
                        var hv = $('#<%= hdnIdCountry.ClientID %>').val();
                        $("#<%= ddlcountry.ClientID %>").val(hv);
                        $('#Count').modal("show");
                    });
                    $('.btn-show').click(function () {
                        window.location.href = '<%= baseUrl %>products.aspx';
                    });
                    $('#<%= lnkCountry.ClientID %>').click(function () {
                        $('#popup').modal('hide');
                        $('#Count').modal('show');
                        return false;
                    });
                    $(".spinner button").click(function (event) {
                        event.preventDefault();
                    });

                    $(".overlay").hide();

                    if (window.history.replaceState) {
                        window.history.replaceState(null, null, window.location.href);
                    }
                });
                function showshoppopup() {
                    $("#popup").modal("show");
                    $('#Count').modal('hide');
                }
                function showSubscribepopup() {
                    $('#popupSubscribe').modal("show");
                    $('#popupGender').modal("hide");
                    $('#btnSubscribe').click(function () {
                        $('#popupSubscribe').modal("hide");
                        $('#popupGender').modal("show");
                    });
                }
                function showGenderpopup() {
                    $('#popupGender').modal("hide");
                }
                function shownowpopup() {
                    $('#Count').modal("show");
                }

                function ShowConfirm(yourDropDown) {
                    //If this is true - Perform your PostBack manually
                    $('#Count').show();
                    var hv = $('#<%= hdnItemInCart.ClientID %>').val();
                    var hw = $('#<%= hdnItemInWishlist.ClientID %>').val();
                    if (hv === "0" && hw === "0") {
                        $('.Count').show();
                        //__doPostBack('ddlcountry', '');
                        return true;

                    }
                    else if (confirm("Changing country will discard your existing cart items.\nDo you want to discard the cart items?")) {
                        //__doPostBack('ddlcountry', '');
                        return true;
                    }
                        //Otherwise the user selected "Cancel" - revert the selection and don't PostBack
                    else {
                        var hv = $('#<%= hdnIdCountry.ClientID %>').val();
                        //yourDropDown.SelectedValue = hv;
                            $(yourDropDown).val(hv);
                            return false;
                        }
                }
                function showAlertMessage(type, message) {
                    $("#alert-message").removeClass('alert alert-success');
                    var msgType = "";
                    if (type == 'success') {
                        $("#alert-message").addClass('alert alert-success')
                        msgType = "<strong>Success! </strong>";
                    }
                    else if (type == 'danger') {
                        $("#alert-message").addClass('alert alert-danger')
                        msgType = "<strong>Error! </strong>";
                    }
                    else if (type == 'warning') {
                        $("#alert-message").addClass('alert alert-warning')
                        msgType = "<strong>Warning! </strong>";
                    }
                    $(".alert-message").html(msgType + message);
                    $("#alert-message").fadeTo(2000, 500).slideUp(500, function () {
                        $("#alert-message").slideUp(500);
                    });
                }
                function ResetMessage() {
                    $("#<%= lblMessage.ClientID %>").text("");
                }
            </script>
